{
  "name": "10x-healthy-meal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "dotenv -e .env -- ng serve",
    "build": "dotenv -e .env -- ng build",
    "watch": "dotenv -e .env -- ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:10x-healthy-meal": "node --env-file=.env dist/10x-healthy-meal/server/server.mjs",
    "dev:server": "dotenv -e .env -- ts-node-dev --project tsconfig.dev-server.json --respawn --transpile-only dev-server/index.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/cdk": "^19.2.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.16",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.9",
    "@supabase/supabase-js": "^2.49.4",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular/cli": "^19.2.9",
    "@angular/compiler-cli": "^19.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "dotenv-cli": "^8.0.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.7.2"
  },
  "targets": {
    "main": {
      "includeNodeModules": {
        "*.scss": true
      }
    }
  }
}
